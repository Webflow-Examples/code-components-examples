{
  "name": "time-machine-backend",
  "main": "./dist/server/entry.mjs",
  "compatibility_date": "2024-04-05",
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "time-machine-db",
      "database_id": "your-database-id-here",
      "migrations_dir": "drizzle"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "ASSETS",
      "id": "assets"
    },
    {
      "binding": "CACHE",
      "id": "your-cache-id-here"
    }
  ]
}
